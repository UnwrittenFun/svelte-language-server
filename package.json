{
    "name": "svelte-language-server",
    "version": "0.6.2",
    "description": "A language server for Svelte",
    "main": "dist/src/index.js",
    "typings": "dist/src/index",
    "scripts": {
        "test": "cross-env TS_NODE_TRANSPILE_ONLY=true mocha --require ts-node/register \"test/**/*.ts\"",
        "build": "tsc",
        "watch": "tsc -w",
        "prepublishOnly": "npm run build"
    },
    "bin": {
        "svelteserver": "bin/server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UnwrittenFun/svelte-language-server.git"
    },
    "keywords": [
        "svelte",
        "vscode",
        "atom",
        "editor"
    ],
    "author": "James Birtles <jameshbirtles@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/UnwrittenFun/svelte-language-server/issues"
    },
    "homepage": "https://github.com/UnwrittenFun/svelte-language-server#readme",
    "devDependencies": {
        "@types/cosmiconfig": "^5.0.0",
        "@types/detect-indent": "^5.0.0",
        "@types/indent-string": "^3.0.0",
        "@types/lodash": "^4.14.116",
        "@types/mocha": "^5.0.0",
        "@types/node": "^10.12.18",
        "@types/prettier": "^1.13.2",
        "@types/sinon": "^7.0.3",
        "cross-env": "^5.2.0",
        "mocha": "^5.1.0",
        "ts-node": "^7.0.1"
    },
    "dependencies": {
        "cosmiconfig": "^5.0.0",
        "detect-indent": "^5.0.0",
        "indent-string": "^3.2.0",
        "lodash": "^4.17.10",
        "prettier": "^1.14.2",
        "sinon": "^7.2.0",
        "source-map": "^0.7.3",
        "svelte": "3.0.0-alpha16",
        "typescript": "^3.0.1",
        "vscode-css-languageservice": "^3.0.9",
        "vscode-emmet-helper": "^1.2.11",
        "vscode-html-languageservice": "^2.1.5",
        "vscode-languageserver": "^5.0.3",
        "vscode-languageserver-types": "^3.10.1",
        "vscode-uri": "^1.0.6"
    }
}
